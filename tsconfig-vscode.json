{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@logi-assets/*": [
                "./build/bin/*"
            ],
            "@logi-base/*": [
                "./build/external/logi_base/*"
            ],
            "@logi-config/*": [
                "./build/bin/*"
            ],
            "@logi-i18n/*": [
                "./build/bin/*"
            ],
            "@logi-icon/*": [
                "./build/bin/*"
            ],
            "@logi-pb/*": [
                "./build/bin/*"
            ],
            "@logi-rs/*": [
                "./build/bin/*"
            ],
            "@logi-sheets/*": [
                "./*"
            ]
        },
        "target": "es5",
        "types": [
            "jasmine",
            "node"
        ]
    },
    "extends": "./tsconfig-bazel.json"
}
